{
  "name": "mockserverpoc",
  "version": "1.0.0",
  "description": "This is a proof of concept about stubby4node",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project ./",
    "start": "node dist/index.js",
    "dev": "ENV_FILE=local tsnd --respawn src/index.ts ",
    "test": "jest",
    "test-ci": "CI=true yarn test --max-workers=2 --coverage",
    "lint": "eslint ./src --ext ts",
    "sonar": "sonar-scanner",
    "prepush": "yarn && yarn lint && yarn test",
    "build:docker": "docker build -t documents .",
    "start:docker": "docker run -dp 8081:3001 -e ENVIRONMENT= -d autentication",
    "prepare": "husky install && chmod ug+x .husky/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcamposcs/MockServerPOC.git"
  },
  "author": "gcampos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gcamposcs/MockServerPOC/issues"
  },
  "homepage": "https://github.com/gcamposcs/MockServerPOC#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-actuator": "^1.7.0",
    "glob": "^7.1.7",
    "helmet": "^4.6.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "log4js": "^6.3.0",
    "passport": "^0.5.0",
    "passport-azure-ad": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-actuator": "^1.8.0",
    "@types/express-formidable": "^1.0.5",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.4",
